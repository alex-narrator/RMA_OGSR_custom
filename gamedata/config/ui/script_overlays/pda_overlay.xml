<?xml version='1.0' encoding="UTF-8"?>
<w>
	<!-- PDA OFF BUTTON-->
	<off_button x="854" y="98" width="30" height="30" hint="pda_off_btn_hint" stretch="1">
		<texture>ui_inGame2_pda_offline</texture>
		<text x="0" y="0" font="letterica18" r="255" g="255" b="255"/>
	</off_button>

	<!-- power level static-->
	<power_level x="855" y="600" width="0" height="0">
		<text align="r" font="letterica18" color="white"/>
	</power_level>
	
	<days_in_zone_text x="128" y="480" width="0" height="0">
		<text align="l" font="letterica18" r="180" g="180" b="180" a="230"/>
	</days_in_zone_text>
	<days_in_zone_count x="363" y="480" width="0" height="0">
		<text align="r" font="letterica18" r="180" g="180" b="180" a="230"/>
	</days_in_zone_count>	
	
	<!-- SKIP TIME-->
	<timer_background x="125" y="515" width="80" height="60">
<!-- 		<text x="0" y="0" font="letterica18" r="255" g="128" b="64" align="c">st_sleep</text> -->
	</timer_background>

	<hour_static x="0" y="0" width="15" height="60">
		<text x="0" y="0" font="letterica18" r="240" g="217" b="182" align="c" vert_align="c"/>
	</hour_static>
	<!-- PLUS, MINUS BUTTON HOURS-->
	<plus_button_h x="0" y="0" width="15" height="20" hR="0" hB="0" hG="0" shadow_offset_x="1" shadow_offset_y="1" push_off_x="1" push_off_y="1">
		<text align="c" font="graffiti22" r="238" g="155" b="23">+</text>
	</plus_button_h>	
	<minus_button_h x="0" y="40" width="15" height="20" hR="0" hB="0" hG="0" shadow_offset_x="1" shadow_offset_y="1" push_off_x="1" push_off_y="1">
		<text align="c" font="graffiti22" r="238" g="155" b="23">-</text>
	</minus_button_h>
	
	<minute_static x="20" y="0" width="15" height="60">
		<text x="0" y="0" font="letterica18" r="240" g="217" b="182" align="c" vert_align="c"/>
	</minute_static>
	<!-- PLUS, MINUS BUTTON MINUTES-->
	<plus_button_m x="0" y="0" width="15" height="20" hR="0" hB="0" hG="0" shadow_offset_x="1" shadow_offset_y="1" push_off_x="1" push_off_y="1">
		<text align="c" font="graffiti22" r="238" g="155" b="23">+</text>
	</plus_button_m>
	<minus_button_m x="0" y="40" width="15" height="20" hR="0" hB="0" hG="0" shadow_offset_x="1" shadow_offset_y="1" push_off_x="1" push_off_y="1">
		<text align="c" font="graffiti22" r="238" g="155" b="23">-</text>
	</minus_button_m>

	<!-- SLEEP WAIT BTN -->
	<sleep_button x="90" y="30" width="90" height="16" hR="0" hB="0" hG="0" shadow_offset_x="1" shadow_offset_y="1" push_off_x="1" push_off_y="1" hint="st_hint_sleep">
		<text align="c" font="graffiti22" r="238" g="155" b="23">st_sleep</text>
	</sleep_button>
	
	<wait_button x="40" y="20" width="40" height="20" hR="0" hB="0" hG="0" shadow_offset_x="1" shadow_offset_y="1" push_off_x="1" push_off_y="1" hint="st_hint_wait">
		<text align="c" vert_align="c" font="graffiti22" r="238" g="155" b="23">st_wait</text>
	</wait_button>
</w>
