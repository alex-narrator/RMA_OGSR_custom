<?xml version='1.0' encoding="UTF-8"?>
<w>
	<!-- PDA OFF BUTTON-->
	<off_button x="854" y="98" width="30" height="30" hint="pda_off_btn_hint" stretch="1">
		<texture>ui_inGame2_pda_offline</texture>
		<text x="0" y="0" font="letterica18" r="255" g="255" b="255"/>
	</off_button>

	<!-- power level static-->
	<power_level x="855" y="600" width="0" height="0">
		<text align="r" font="letterica18" color="white"/>
	</power_level>
	
	<money x="695" y="600" width="0" height="0">
		<text align="r" font="letterica18" color="white"/>
	</money>	
	
	<!-- SKIP TIME-->
	<timer_frame x="435" y="497" width="120" height="90">
		<texture>ui_icons_PDA_tooltips</texture>
	</timer_frame>
	
	<timer_header x="20" y="2" width="60" height="20">
		<text x="0" y="0" font="arial_21" align="c" vert_align="c">st_wait_timer</text>
	</timer_header>
	
	<separator_static x="15" y="24" width="15" height="60">
		<text x="0" y="-2" font="arial_21" r="240" g="217" b="182" align="c" vert_align="c">:</text>
	</separator_static>

	<hour_static x="5" y="25" width="15" height="60">
		<text x="0" y="-2" font="arial_21" r="240" g="217" b="182" align="c" vert_align="c"/>
	</hour_static>
	<!-- PLUS, MINUS BUTTON HOURS-->
	<plus_button_h x="2" y="0" width="12" height="15" stretch="1" hint="st_inc_hour">
		<texture>ui_beltbut_plus</texture>
		<!-- <text x="0" y="0" align="c" vert_align="c" font="arial_21">+</text> -->
	</plus_button_h>	
	<minus_button_h x="2" y="40" width="12" height="15" stretch="1" hint="st_dec_hour">
		<texture>ui_beltbut_min</texture>
		<!-- <text align="c" vert_align="c" font="arial_21">-</text> -->
	</minus_button_h>
	
	<minute_static x="25" y="25" width="15" height="60">
		<text x="0" y="-2" font="arial_21" r="240" g="217" b="182" align="c" vert_align="c"/>
	</minute_static>
	<!-- PLUS, MINUS BUTTON MINUTES-->
	<plus_button_m x="2" y="0" width="12" height="15" stretch="1" hint="st_inc_min">
		<texture>ui_beltbut_plus</texture>
		<!-- <text x="0" y="0" align="c" vert_align="c" font="arial_21">+</text> -->
	</plus_button_m>
	<minus_button_m x="2" y="40" width="12" height="15" stretch="1" hint="st_dec_min">
		<texture>ui_beltbut_min</texture>
		<!-- <text align="c" vert_align="c" font="arial_21">-</text> -->
	</minus_button_m>

	<!-- SLEEP WAIT BTN -->
	<sleep_button x="90" y="30" width="90" height="16" hR="0" hB="0" hG="0" shadow_offset_x="1" shadow_offset_y="1" push_off_x="1" push_off_y="1" hint="st_hint_sleep">
		<text align="c" font="graffiti22" r="238" g="155" b="23">st_sleep</text>
	</sleep_button>
	
	<wait_button x="50" y="31" width="50" height="20" stretch="1" hint="st_hint_wait">
		<texture>ui_button_ordinary</texture>
		<text x="0" y="-1" align="c" vert_align="c" font="arial_21">st_wait</text>
	</wait_button>
	
	<clear_button x="50" y="56" width="50" height="20" stretch="1" hint="st_hint_clear">
		<texture>ui_button_ordinary</texture>
		<text x="0" y="-1" align="c" vert_align="c" font="arial_21">st_clear</text>
	</clear_button>
</w>
